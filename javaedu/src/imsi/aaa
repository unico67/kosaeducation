
			ObjectMapper mapper = new com.fasterxml.jackson.databind.ObjectMapper();
			List<HashMap<String, Object>> listOfMaps = mapper.readValue(req, new TypeReference<ArrayList<HashMap<String, Object>>>() {});
			System.out.println(listOfMaps.size());
